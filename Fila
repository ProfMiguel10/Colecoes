import java.util.LinkedList;

public class Fila {
	public static void main(String[] args) {
		LinkedList<String> fila = new LinkedList<String>();
		fila.add("Anderson");
		fila.add("Jos√© Luiz");
		fila.add("Marcelo");
		fila.add("Beatriz");
		int tam = fila.size();

		for (int i = 0; i < tam; i++) {
			System.out.println(fila.getFirst());
			fila.removeFirst();
		}
	}
}
