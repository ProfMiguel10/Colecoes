package array;

public class ImplArray {

	public static void main(String[] args) {
		String []  aStr = new String[10];
		aStr[0]="Diogo";
		aStr[1]="Igor";
		aStr[2]="Núbia";
		aStr[3]="Adriano";
		aStr[4]="Stella";
		aStr[5]="Iolanda";
		aStr[6]="Miguel";
		aStr[7]="André";
		aStr[8]="Elaine";
		aStr[9]="Pellon";
		aStr[10]="Thiago";// exceção
	}
}
